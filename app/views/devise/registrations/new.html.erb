<!---New Form-->
<div class="container" id="signup">
<h2 id="new_user">Sign up</h2>

<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

  <div class="field">

    <%= f.text_field :first_name, autofocus: true, :class=>"form-control",:placeholder=>"First name" %>
  </div>
  

  <div class="field">

    <%= f.text_field :last_name, :class=>"form-control",:placeholder=>"Last name" %>
  </div>

  <div class="field">
    <!--<#%= f.label :email %><br />-->
    <%= f.email_field :email, :class=>"form-control",:placeholder=>"Email" %>
  </div>

  <div class="field">
      <!--<#%= f.label :password %>-->
    <% if @minimum_password_length %>
    <em>min. <%= @minimum_password_length %> characters </em>
    <% end %><br />
    <%= f.password_field :password, autocomplete: "off",:class=>"form-control",:placeholder=>"Password" %>
  </div>

  <div class="field">
      <!--<%= f.label :password_confirmation %><br />-->
    <%= f.password_field :password_confirmation, autocomplete: "off",:class=>"form-control",:placeholder=>"Repeat password" %>
  </div>

  <div class="actions">
    <%= f.submit "Sign up",:class=>"btn btn-warning", :style=>"width:100%" %>
  </div>
<% end %>

<p>Already have an account?</p>

<%= render "devise/shared/links" %>

</div>

